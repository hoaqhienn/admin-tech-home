{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/error/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/properties/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/residents/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/more/(.*)",
      "dest": "/index.html"
    }
  ]
}